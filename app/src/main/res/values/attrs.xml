<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingItemView">
        <attr name="titles" format="string"/>
        <attr name="bgType">
            <enum name="start" value="0"/>
            <enum name="end" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="GuideIndicator">
        <!-- 当前步骤索引 -->
        <attr name="step" format="integer" min="1"/>
        <!-- 向导总步骤数 -->
        <attr name="total" format="integer" min="1"/>
    </declare-styleable>

</resources>